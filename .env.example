# Razorpay Test Keys
# Get these from: https://dashboard.razorpay.com/app/keys
RAZORPAY_TEST_KEY_ID=rzp_test_YOUR_KEY_ID_HERE
RAZORPAY_TEST_KEY_SECRET=YOUR_TEST_KEY_SECRET_HERE

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/lovenest

# Email Configuration (for sending receipts)
# For Gmail: Enable "Less secure app access" or use App Password
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-app-password

# Server Configuration
NODE_ENV=development
PORT=3000

# JWT Secret (for authentication)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
